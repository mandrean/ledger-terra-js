  },
  "keywords": [
    "Ledger",
    "Javascript",
    "Tendermint",
    "Cosmos",
    "Terra"
  ],
  "author": "ZondaX GmbH",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/terra-project/ledger-terra-js/issues"
  },
  "homepage": "https://github.com/terra-project/ledger-terra-js",
  "dependencies": {
    "@ledgerhq/hw-transport": "^5",
    "bech32": "^1.1.3",
    "ripemd160": "^2.0.2",
    "node-hid": "^2.1.1"
  },
  "devDependencies": {
    "@ledgerhq/hw-transport-node-hid": "^5",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/hw-transport-webusb": "^5",
    "@ledgerhq/logs": "^5",
    "@types/jest": "^26.0.0",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "^4.1.1",
    "bip32": "^2",
    "bip39": "3.0.2",
    "debug": "^4.1.1",
    "eslint": "^6",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "jest": "^26",
    "jest-serial-runner": "^1.1.0",
    "prettier": "^2",
    "secp256k1": "^3",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "scripts": {
    "build": "tsc",
    "test:unit": "jest -c jest.config.unit.js",
    "test:integration": "jest -c jest.config.integration.js --runInBand --detectOpenHandles",
    "serve": "openssl req -nodes -new -x509 -config certs/openssl.cnf -keyout certs/server.key -out certs/server.cert; openssl x509 -in certs/server.cert -out certs/cert.pem -outform PEM; vue-cli-service serve src/main.js"
  },
  "moduleDirectories": [
    "node_modules",
    "dist"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
